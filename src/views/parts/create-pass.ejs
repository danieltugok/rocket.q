<section>
  <h2>Crie sua propria sala</h2>
  <form action="/create-room" method="POST">
    <label class="src-only" for="room-id">Insira uma senha</label>
    <input
      type="password"
      name="password"
      id="room-pass"
      placeholder="Codigo da sala"
    />
    <button>
      <img src="images/users_white.svg" alt="Entrar na sala" />
      Criar sala
    </button>
  </form>
</section>
